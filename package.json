{
  "name": "base_bevr-stack",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && git clean -xdf node_modules",
    "dev": "turbo run dev",
    "lint": "biome lint",
    "format": "biome check --write",
    "format:check": "biome check",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "db:push": "turbo run push --filter=@repo/db",
    "db:generate": "turbo run prismagenerate --filter=@repo/db",
    "db:deploy": "turbo run deploy --filter=@repo/db",
    "ui:add": "turbo run ui:add --filter=@repo/ui --"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@effect/language-service": "^0.53.3",
    "turbo": "^2.5.8",
    "typescript": "5.9.3",
    "ultracite": "5.6.4"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "bun@1.3.0",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "eslint": "^9.34.0",
      "effect": "3.18.4",
      "@effect/ai": "^0.31.1",
      "@effect/ai-openai": "^0.34.0",
      "@effect/experimental": "^0.56.0",
      "@effect/platform": "0.92.1",
      "@effect/platform-bun": "0.81.1",
      "@effect/platform-browser": "^0.71.0",
      "@effect/rpc": "0.71.1",
      "@effect/language-service": "^0.45.1",
      "@effect-atom/atom-react": "^0.3.3",
      "better-auth": "^1.3.27",
      "react": "^19.2.0",
      "react-dom": "^19.2.0",
      "clsx": "^2.1.1",
      "class-variance-authority": "^0.7.1",
      "lucide-react": "^0.546.0",
      "tailwindcss": "^4.1.14",
      "tailwind-merge": "^3.3.1",
      "@types/react": "^19.2.2",
      "@types/react-dom": "^19.2.2",
      "vite": "^7.1.10",
      "typescript": "5.9.2"
    }
  },
  "dependencies": {
    "@tanstack/react-router": "^1.133.22",
    "@tanstack/react-start": "^1.133.22"
  }
}
